{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
    "jest"
  ],
  "rules": {
    "jest/no-large-snapshots": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/require-default-props": 0,
    "arrow-parens": 0,
    "jsx-quotes": 0,
    "max-len": 0,
    "no-plusplus": 0,
    "operator-linebreak": 0
  },
  "env": {
    "es6": true,
    "jest/globals": true
  }
}
